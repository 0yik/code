<odoo>
	<record id="time_frame_form" model="ir.ui.view">
	    <field name="name">time.frame.form</field>
	    <field name="model">time.frame</field>
	    <field name="arch" type="xml">
	        <form string="Time Frame">
	        	<sheet>
	        		<group colspan="4">
	        			<group>
		                	<field name="start"/>
	                	</group>
	                	<group>
		                	<!-- <field name="finish" options="{'no_create_edit': True,'no_create': True, 'no_open': True, 'limit': 24}"/> -->
		                	<field name="finish"/>
		                </group>
	                </group>
	               
	                <group>
	                	<group>
	                	<field name="price"/>
	                	<field name="qty"/>
	                </group>
	                </group>
	           	               <!--  <group>
	                <button type="object" name="time_frame_action" string="ADD" class="oe_highlight"/>
	                </group> -->
	            </sheet>
	        </form>
	    </field>
    </record>

    <record id="time_frame_tree" model="ir.ui.view">
        <field name="name">time.frame.tree</field>
        <field name="model">time.frame</field>
        <field name="arch" type="xml">
            <tree string="Time Frame">
            	
            	<field name="start"/>
            	<field name="finish"/>
            	<field name="price"/>
            	<field name="qty"/>
            
            </tree>
        </field>
    </record>
    
    <record id="action_time_frame_form" model="ir.actions.act_window">
            <field name="name">Time Frame</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">time.frame</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        parent="point_of_sale.menu_point_config_product"
        action="action_time_frame_form"
        id="menu_time_frame"
        sequence="31"
        groups="pos_delivery_timeframe_management.time_from_management_group" />
</odoo>