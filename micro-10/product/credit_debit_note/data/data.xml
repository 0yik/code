<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="invoice_debit_note" model="ir.sequence">
        <field name="name">Invocie Debit Note</field>
        <field name="code">invocie.debit.note</field>
        <field name="prefix">CDN/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="invoice_credit_note" model="ir.sequence">
        <field name="name">Invocie Credit Note</field>
        <field name="code">invocie.credit.note</field>
        <field name="prefix">CCN/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="bill_debit_note" model="ir.sequence">
        <field name="name">Purchase Bill Debit Note</field>
        <field name="code">bill.debit.note</field>
        <field name="prefix">VDN/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="bill_credit_note" model="ir.sequence">
        <field name="name">Purchase Bill Credit Note</field>
        <field name="code">bill.credit.note</field>
        <field name="prefix">VCN/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>
