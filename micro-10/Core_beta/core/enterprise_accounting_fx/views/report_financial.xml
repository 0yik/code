<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="report_financial_body_extend" inherit_id="enterprise_accounting_report.report_financial_body">
        <xpath expr="//div/div[5]/table/thead/tr[1]/t" position="replace">
            <t t-foreach="context.get_special_date_line_names()" t-as="column_name">
                <th class="text-center" t-raw="column_name" colspan='3'/>
            </t>
        </xpath>
    </template>

</odoo>