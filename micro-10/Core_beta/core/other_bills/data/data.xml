<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="other_invoice_sequence" model="ir.sequence">
        <field name="name">Ohter Invocie Sequence</field>
        <field name="code">other.invocie.sequence</field>
        <field name="prefix">OTHINV/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="other_bills_sequence" model="ir.sequence">
        <field name="name">Ohter Bills Sequence</field>
        <field name="code">other.bills.sequence</field>
        <field name="prefix">OTHBILL/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>
