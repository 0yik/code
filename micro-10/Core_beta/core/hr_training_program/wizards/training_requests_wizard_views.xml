<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="training_requests_wizard_form_view">
            <field name="name">training.requests.wizard.form</field>
            <field name="model">training.requests.wizard</field>
            <field name="arch" type="xml">
                <form string="Training Conducts">
                    <sheet>
                        <group>
                            <field name="request_id" invisible="True"/>
                            <field name="conducts_ids"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_confirm" type="object"
                                string="Confirm" class="oe_highlight"/>
                        or
                        <button special="cancel" string="Cancel"/>
                    </footer>

                </form>
            </field>
        </record>
    </data>
</odoo>