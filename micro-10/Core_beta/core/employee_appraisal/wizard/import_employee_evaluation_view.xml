<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

       	<record id="employee_evaluation_import_view" model="ir.ui.view">
    	    <field name="name">employee.evaluation.import.form</field>
       	    <field name="model">employee.evaluation.import</field>
       	    <field name="arch" type="xml">
	            <form>
	            	<field name="datas" filename="datas_fname"/>
	            	<field name="datas_fname" readonly = "1" invisible="1"/>
				<footer>
				<button name="import_data" string="Import" type="object"  colspan="2" class="oe_highlight" />
				
				or
				
				<button name="cancel" string="cancel" class="oe special oe_link"/>
				</footer>
				</form>
            </field>
	</record>

	<record id="employee_evaluation_import_action" model="ir.actions.act_window">
            <field name="name">Import Employee Evaluation</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">employee.evaluation.import</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
      	</record>

	</data>
</odoo>
