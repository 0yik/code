<odoo>
    <data>
        <record id="booking_reschedule_form_view" model="ir.ui.view">
            <field name="name">booking.reschedule.wizard</field>
            <field name="model">booking.reschedule.wizard</field>
            <field name="arch" type="xml">
                <form string="Analytic Account Report">
                    <group>
                        <group>
                            <field name="current_start_date"/>
                            <field name="current_end_date"/>
                        </group>
                        <group>
                            <field name="new_start_date"/>
                            <field name="new_end_date"/>
                        </group>
                    </group>
                    <footer>
                        <button name="reschedule_confirm" string="Confirm" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_highlight" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_booking_reschedule_report">
            <field name="name">Reschedule Booking Facility Order</field>
            <field name="res_model">booking.reschedule.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="booking_reschedule_form_view"/>
            <field name="target">new</field>
        </record>
    </data>
</odoo>