<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<record model="ir.cron" id="dev_task_state_to_done">
        	<field name="name">Developer state to done</field>
        	<field name="interval_number">1</field>
        	<field name="interval_type">days</field>
        	<field name="nextcall" eval="(DateTime.now() + timedelta(days=0)).strftime('%Y-%m-%d 17:00:00')" />
        	<field name="numbercall">-1</field>
        	<field name="doall" eval="False"/>
        	<field name="model" eval="'developer.task.list'"/>
        	<field name="function" eval="'set_as_done'"/>
        	<field name="args" eval="'()'" />
    	</record>
    </data>
</odoo>