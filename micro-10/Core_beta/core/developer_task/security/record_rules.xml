<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
		 <!-- Project Head Group. It will have master access to all -->
        <record id="group_project_head" model="res.groups">
            <field name="name">Project Head</field>
            <field name="category_id" ref="base.module_category_project_management"/>
            <field name="implied_ids" eval="[(4, ref('project.group_project_manager'))]"/>
        </record>

		<!-- Developer Access Group -->
        <record id="group_project_developer_access" model="res.groups">
	        <field name="name">Developer</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        
        <!-- Developer only can see own tsks -->
	    <record id="developer_user_task_access_rule" model="ir.rule">
	        <field name="name">Developer Tasks : Developer User Access</field>
	        <field name="model_id" ref="model_developer_task_list"/>
	        <field name="domain_force">[('developer_id','=',user.id)]</field>
	        <!--<field eval="1" name="perm_read"/>
			<field eval="1" name="perm_create"/>
			<field eval="1" name="perm_write"/>
			<field eval="1" name="perm_unlink"/> -->
			<field name="groups" eval="[(4,ref('developer_task.group_project_developer_access'))]"/>			       
	    </record>        
	    
	</data>
</odoo>