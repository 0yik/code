<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="view_package_label_new" model="ir.ui.view">
		<field name="name">package.label.new.form</field>
		<field name="model">package.label</field>
		<field name="arch" type="xml">
		    <form string="package label Yes">
				<group>
					<field name="pack_opration" widget="one2many" nolabel="1">
		        		<tree editable="bottom">
			        		<field name="custom_product_id" readonly="1"/>
			        		<field name="lot_custom_number" readonly="1"/>
			        		<field name="do_quantity" readonly="1"/>
			        		<field name="no_of_labels"/>
			        		<field name="label_quantity"/>
			        		<field name="line" invisible="1"/>
		        		</tree>
		        	</field>
		        </group>
		        <footer>
		            <button string="Print" name="print_data" type="object" class="btn-primary"/>
		            <button string="Cancel" name="cancel" type="object" class="btn-primary"/>
		        </footer>
		    </form>
		</field>
    </record>
</odoo>    
