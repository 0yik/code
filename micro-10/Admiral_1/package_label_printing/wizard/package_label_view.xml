<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_package_label" model="ir.ui.view">
        <field name="name">package.label.view.form</field>
        <field name="model">package.label</field>
        <field name="arch" type="xml">
            <form string="package label">
                <group>
                    <p>
                        Do you want to print package label?
                    </p>
                </group>
                <footer>
                    <button name="yes" string="Yes" type="object" class="btn-primary"/>
                    <button name="no" type="object" string="No" class="btn-primary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
