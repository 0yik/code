<odoo>
    <data>
        <template id="report_manufacturing_order">
            <t t-call="report.html_container">
                <div class="page">
                    <t t-foreach="docs" t-as="doc">
                        <div>
                            <p style="font-size:130px;text-align:center" t-esc="doc.name"/>
                        </div>
                    </t>
                </div>
            </t>
        </template>

        <record id="manufacturing_order_qweb_paperformat" model="report.paperformat">
            <field name="name">Manufacturing Order</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">10.00</field>
            <field name="margin_bottom">30.00</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">30</field>
            <field name="dpi">90</field>
        </record>

        <report
                id="action_report_manufacturing_order"
                model="mrp.production"
                string="Manufacturing Order Number"
                report_type="qweb-pdf"
                name="admiral_modifier_printout.report_manufacturing_order"
                file="admiral_modifier_printout.report_manufacturing_order"
        />

        <record id="action_report_manufacturing_order" model="ir.actions.report.xml">
            <field name="paperformat_id" ref="manufacturing_order_qweb_paperformat"/>
        </record>
    </data>
</odoo>