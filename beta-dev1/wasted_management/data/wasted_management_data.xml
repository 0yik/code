<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_location_wasted" model="stock.location">
            <field name="name">Wasted</field>
            <field name="location_id" ref="stock.stock_location_locations_virtual"/>
            <field name="wasted_location">True</field>
            <field name="usage">inventory</field>
            <field name="company_id"></field>
    </record>


    <record id="sequence_scrap_manufacturing" model="ir.sequence">
        <field name="name">Wasted Management</field>
        <field name="code">wasted.management</field>
        <field name="prefix">WS/</field>
        <field name="padding">5</field>
    </record>

</odoo>
