<odoo>
    <data>
        <record id="booking_setting_form_view_inherit" model="ir.ui.view">
            <field name="name">Booking Setting View Inherit</field>
            <field name="model">booking.settings</field>
            <field name="inherit_id" ref="booking_service_V2.booking_service_configuration"/>
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/group" position="after">
                    <group string="Restriction Validate">
                        <group>
                            <field name="block_by_worker"/>
                            <field name="blook_by_equipment"/>
                        </group>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>