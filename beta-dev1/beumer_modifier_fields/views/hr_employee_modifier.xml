<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--Employee race-->
        <record model="ir.ui.view" id="hr_employee_race_form_view">
            <field name="name">hr.employee.race.tree</field>
            <field name="model">hr.employee.race</field>
            <field name="arch" type="xml">
                <form string="Employee Race Form">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="hr_employee_race_tree_view">
            <field name="name">hr.employee.race.tree</field>
            <field name="model">hr.employee.race</field>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <tree string="Employee Race list">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="hr_employee_race_action">
            <field name="name">Employee Race</field>
            <field name="res_model">hr.employee.race</field>
        </record>


        <menuitem id="hr_employee_race_menu" name="Employee Race"
                  parent="hr.menu_human_resources_configuration"
                  action="hr_employee_race_action"/>

        <!--Employee Religion-->
