<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record id="student_enrolment_form_view" model="ir.ui.view">
            <field name="name">monthly.enrolment_form.form</field>
            <field name="model">monthly.enrolment.wizard</field>
            <field name="arch" type="xml">
                <form string="Student Enrolement form">
                    <group>
                    	<group>
                        	<field name='date_selection'/> 
                        </group>
                        <footer>
                        	<button name="check_report" string="Print" type="object" class="btn-primary"/>
                        	<button string="Cancel" class="btn-default" special="cancel"/>
                        </footer>
                    </group>
                </form>
            </field>
        </record>

		<!-- Action of Terminate Student Wizard -->
        <record id="action_monthly_enrolment_wizard" model="ir.actions.act_window">
            <field name="name">Monthly Enrolment Wizard</field>
            <field name="res_model">monthly.enrolment.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        
