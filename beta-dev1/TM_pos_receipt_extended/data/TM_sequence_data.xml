<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for sale.order -->
        <record id="sale.seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="customer_invoice" model="ir.sequence">
           <field name="name">customer invoices sequence</field>
           <field name="code">account.invoice</field>
           <field name="prefix"></field>
           <field name="number_next" eval="1"/>
           <field name="number_increment" eval="1"/>
           <field name="use_date_range" eval="True"/>
           <field name="company_id" eval="False"/>
           <field name="padding">5</field>
        </record>

        <record id="customer_invoice_supplier" model="ir.sequence">
           <field name="name">customer invoices sequence supplier</field>
           <field name="code">account.invoice.supplier</field>
           <field name="prefix"></field>
           <field name="number_next" eval="1"/>
           <field name="number_increment" eval="1"/>
           <field name="use_date_range" eval="True"/>
           <field name="company_id" eval="False"/>
           <field name="padding">5</field>
        </record>

        <record id="credit_debit_note.invoice_debit_note" model="ir.sequence">
            <field name="name">Invocie Debit Note</field>
            <field name="code">invocie.debit.note</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>
        <record id="credit_debit_note.invoice_credit_note" model="ir.sequence">
            <field name="name">Invocie Credit Note</field>
            <field name="code">invocie.credit.note</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="pos_delivery_order_sequence" model="ir.sequence">
            <field name="name">POS Delivery Order</field>
            <field name="code">pos.delivery.order</field>
            <field name="prefix">POS</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>
    </data>
</odoo>
