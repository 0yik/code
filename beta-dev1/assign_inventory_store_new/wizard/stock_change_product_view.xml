<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_stock_change_qty_form" model="ir.ui.view">
            <field name="name">stock.change.product.qty.form</field>
            <field name="model">stock.change.product.qty</field>
            <field name="inherit_id" ref="stock.view_change_product_quantity"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='new_quantity']" position="after">
                    <field name="qty_warehouse_new"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
