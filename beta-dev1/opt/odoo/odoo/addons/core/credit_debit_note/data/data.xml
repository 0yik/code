<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="0">
    <record id="invoice_debit_note" model="ir.sequence">
        <field name="name">Invocie Debit Note</field>
        <field name="code">invocie.debit.note</field>
        <field name="prefix">DN/%(year)s/</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>
	<record id="invoice_credit_note" model="ir.sequence">
        <field name="name">Invocie Credit Note</field>
        <field name="code">invocie.credit.note</field>
        <field name="prefix">CN/%(year)s/</field>
        <field name="padding">3</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False" />
    </record>
</data>
</odoo>
