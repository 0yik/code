<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="sale.seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order</field>
            <field name="prefix">SO</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for stock.picking -->
        <record id="stock_picking_out" model="ir.sequence">
            <field name="name">Stock Picking Out</field>
            <field name="code">stock.picking</field>
            <field name="prefix">SOUT%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="stock_picking_in" model="ir.sequence">
            <field name="name">Stock Picking In</field>
            <field name="code">stock.picking</field>
            <field name="prefix">SIN%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
