<odoo>
    <data>
        <record model="ir.ui.view" id="booking_team_product_form_view">
            <field name="name">booking.team.product.form</field>
            <field name="model">booking.team.product</field>
            <field name="arch" type="xml">
                <form string="Team Form">
                    <sheet>
                        <group>
                            <field name="product_id"/>
                            <field name="lot_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="booking_team_product_tree_view">
            <field name="name">booking.team.product.tree</field>
            <field name="model">booking.team.product</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Team Form">
                    <field name="product_id"/>
                    <field name="lot_id"/>
                </tree>
            </field>
        </record>
        <!--employee-->
        <record model="ir.ui.view" id="booking_team_employee_form_view">
            <field name="name">booking.team.employee.form</field>
            <field name="model">booking.team.employee</field>
            <field name="arch" type="xml">
                <form string="Team Form">
                    <sheet>
                        <group>
                            <field name="employee_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="booking_team_employee_tree_view">
            <field name="name">booking.team.employee.tree</field>
            <field name="model">booking.team.employee</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Team Form">
                    <field name="employee_id"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>