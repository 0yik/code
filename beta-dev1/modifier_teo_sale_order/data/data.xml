<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="decimal_product_sale" model="decimal.precision">
			<field name="name">Product Sale Qty</field>
			<field name="digits">0</field>
		</record>

		<record id="sale.seq_sale_order" model="ir.sequence">
			<field name="name">Sales Order</field>
			<field name="code">sale.order</field>
			<field name="prefix">SO%(y)s%(month)s</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False" />
		</record>

	</data>
</odoo>
