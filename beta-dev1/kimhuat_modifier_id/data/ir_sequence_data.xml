<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_company" model="ir.sequence">
            <field name="name">Company</field>
            <field name="code">res.partner.company</field>
            <field name="prefix">C</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_customer" model="ir.sequence">
            <field name="name">Supplier</field>
            <field name="code">res.partner.supplier</field>
            <field name="prefix">S</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
