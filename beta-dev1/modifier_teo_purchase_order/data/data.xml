<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="decimal_price_unit" model="decimal.precision">
			<field name="name">Price Unit</field>
			<field name="digits">4</field>
		</record>

		<record id="purchase.seq_purchase_order" model="ir.sequence">
			<field name="name">Purchase Order</field>
			<field name="code">purchase.order</field>
			<field name="prefix">PO%(y)s%(month)s</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False" />
		</record>

	</data>
</odoo>
