<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order.aik</field>
            <field name="prefix">PO-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_quotation" model="ir.sequence">
            <field name="name">Quotation Order</field>
            <field name="code">quotaion.order</field>
            <field name="prefix">QN-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
		<record id="seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order.aik</field>
            <field name="prefix">SO-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_delivery" model="ir.sequence">
            <field name="name">Delivery Order</field>
            <field name="code">stock.picking</field>
            <field name="prefix">DO-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_pos_order" model="ir.sequence">
            <field name="name">POS Cash Bill</field>
            <field name="code">pos.order.format</field>
            <field name="prefix">CS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_credit_note" model="ir.sequence">
            <field name="name">Credit Note</field>
            <field name="code">credit.note</field>
            <field name="prefix">CN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_debit_note" model="ir.sequence">
            <field name="name">Debit Note</field>
            <field name="code">debit.note</field>
            <field name="prefix">DN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>