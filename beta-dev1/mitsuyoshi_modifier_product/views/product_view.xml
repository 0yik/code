<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_product_template_inherited_custom">
            <field name="name">product.template.inherited.form.custom</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <field name="categ_id" position="after">
                    <field name="part_name"/>
                    <field name="pmb_no"/>
                    <field name="customer_pmb_no"/>
                    <!--<field name="mit_pmb_no"/>-->
                    <field name="sup_no"/>
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="view_product_template_inherited_custom_tree">
            <field name="name">product.template.inherited.tree.custom</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <field name="categ_id" position="after">
                    <field name="part_name"/>
                    <field name="pmb_no"/>
                    <field name="customer_pmb_no"/>
                    <!--<field name="mit_pmb_no"/>-->
                    <field name="sup_no"/>
                </field>
            </field>
        </record>
    </data>
</odoo>